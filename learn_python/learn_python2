
import psycopg2

conn = psycopg2.connect(
    host = 'localhost',
    database = 'Local Library',
    user = 'postgres',
    password = 'Oluwaseyi@1',
    port = 5432 
)

#cursor is used to execute sql queries
cur = conn.cursor()

cur.execute('select * from books')

# Fetch all rows from the result of the query
rows = cur.fetchall()

# Print the result
for row in rows:
    print(row)

# Close the cursor and connection
cur.close()


conn.close()